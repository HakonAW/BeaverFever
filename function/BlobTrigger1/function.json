{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "beaverupload/data/{inName}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputBlob",
      "direction": "out",
      "path": "beaverupload/copy/{outName}",
      "connection": "AzureWebJobsStorage",
      "dataType": "string"
    }
  ]
}
